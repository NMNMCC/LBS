{
	"nodeModulesDir": "none",
	"imports": {
		"hono": "npm:hono@^4.9.1",
		"@effect/cli": "npm:@effect/cli@^0.69.0",
		"@effect/platform-node": "npm:@effect/platform-node@^0.96.0",
		"@std/assert": "jsr:@std/assert@^1.0.14",
		"effect": "npm:effect@^3.17.7",
		"gel": "npm:gel@^2.1.1",
		"swr": "npm:swr@^2.3.6",
		"uuid": "npm:uuid@^11.1.0",
		"zod": "npm:zod@^4.0.17"
	},
	"workspace": [
		"./package/*"
	],
	"lock": false,
	"fmt": {
		"useTabs": true,
		"useBraces": "always",
		"indentWidth": 4,
		"newLineKind": "lf",
		"trailingCommas": "onlyMultiLine"
	},
	"lint": {
		"rules": {
			"exclude": [
				"no-explicit-any"
			]
		}
	}
}
